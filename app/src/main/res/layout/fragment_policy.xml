<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BottomNavigation.Settings.PolicyFragment"
    android:background="@color/white">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="@dimen/vertical_default_padding"
        android:layout_marginHorizontal="@dimen/horizontal_default_padding"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            android:paddingVertical="@dimen/vertical_default_padding">

            <TextView
                android:layout_width="@dimen/policy_title_width"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/listing_division"
                android:gravity="left"
                android:maxLines="2"
                android:text="Policy No."
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title" />

            <TextView
                android:id="@+id/txt_detail_policyNum"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="1"
                android:layout_marginTop="@dimen/listing_division"
                android:textAllCaps="true"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title"
                android:textStyle="bold"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            android:paddingVertical="@dimen/vertical_default_padding">
            <TextView
                android:layout_width="@dimen/policy_title_width"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="2"
                android:layout_marginTop="@dimen/listing_division"
                android:text="Premium Amount Due"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title" />

            <TextView
                android:id="@+id/txt_detail_premiumAmountDue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="1"
                android:layout_marginTop="@dimen/listing_division"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title"
                android:textStyle="bold"
                android:textAllCaps="true"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            android:paddingVertical="@dimen/vertical_default_padding">

            <TextView
                android:layout_width="@dimen/policy_title_width"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="2"
                android:layout_marginTop="@dimen/listing_division"
                android:text="Premium Outstanding"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title" />

            <TextView
                android:id="@+id/txt_detail_premiumOutstanding"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="1"
                android:layout_marginTop="@dimen/listing_division"
                android:textAllCaps="true"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title"
                android:textStyle="bold"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            android:paddingVertical="@dimen/vertical_default_padding">
            <TextView
                android:layout_width="@dimen/policy_title_width"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="2"
                android:layout_marginTop="@dimen/listing_division"
                android:text="Payment Due Date"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title" />

            <TextView
                android:id="@+id/txt_detail_paymentDueDate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:maxLines="1"
                android:layout_marginTop="@dimen/listing_division"
                android:textAllCaps="true"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title"
                android:textStyle="bold"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            android:paddingVertical="@dimen/vertical_default_padding">
            <TextView
                android:layout_width="@dimen/policy_title_width"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="2"
                android:layout_marginTop="@dimen/listing_division"
                android:text="Insurance Period"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title" />

            <TextView
                android:id="@+id/txt_detail_insurancePeriod"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="2"
                android:layout_marginTop="@dimen/listing_division"
                android:textAllCaps="true"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title"
                android:textStyle="bold"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            android:paddingVertical="@dimen/vertical_default_padding">
            <TextView
                android:layout_width="@dimen/policy_title_width"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="2"
                android:layout_marginTop="@dimen/listing_division"
                android:text="Effective Date"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title" />

            <TextView
                android:id="@+id/txt_detail_effectiveDate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="1"
                android:layout_marginTop="@dimen/listing_division"
                android:textAllCaps="true"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title"
                android:textStyle="bold"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            android:paddingVertical="@dimen/vertical_default_padding">
            <TextView
                android:layout_width="@dimen/policy_title_width"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="2"
                android:layout_marginTop="@dimen/listing_division"
                android:text="Policy Holder"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title" />

            <TextView
                android:id="@+id/txt_detail_policyHolder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="3"
                android:layout_marginTop="@dimen/listing_division"
                android:textAllCaps="true"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title"
                android:textStyle="bold"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            android:paddingVertical="@dimen/vertical_default_padding">
            <TextView
                android:layout_width="@dimen/policy_title_width"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="2"
                android:layout_marginTop="@dimen/listing_division"
                android:text="Address"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title" />

            <TextView
                android:id="@+id/txt_detail_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="5"
                android:ellipsize="end"
                android:layout_marginTop="@dimen/listing_division"
                android:textAllCaps="true"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title"
                android:textStyle="bold"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            android:paddingVertical="@dimen/vertical_default_padding">
            <TextView
                android:layout_width="@dimen/policy_title_width"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="2"
                android:layout_marginTop="@dimen/listing_division"
                android:text="Reference No."
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title" />

            <TextView
                android:id="@+id/txt_detail_referenceNumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="1"
                android:layout_marginTop="@dimen/listing_division"
                android:textAllCaps="true"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title"
                android:textStyle="bold"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clickable="true"
            android:paddingVertical="@dimen/vertical_default_padding">
            <TextView
                android:layout_width="@dimen/policy_title_width"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="2"
                android:layout_marginTop="@dimen/listing_division"
                android:text="STNC Date"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title" />

            <TextView
                android:id="@+id/txt_detail_stncDate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:maxLines="1"
                android:layout_marginTop="@dimen/listing_division"
                android:textAllCaps="true"
                android:textColor="@color/fpg_gray"
                android:textSize="@dimen/setting_title"
                android:textStyle="bold"/>
        </LinearLayout>

    </LinearLayout>

</FrameLayout>